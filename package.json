{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "description": "",
  "main": "src/server",
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rm -rf ./dist",
    "dev": "npm run watch & npm run start",
    "lint": "prettier-standard 'src/**/*.{js,jsx,ts,tsx}'",
    "prod": "NODE_ENV=production npm run build && NODE_ENV=production npm run start",
    "profile": "clinic doctor --on-port='autocannon -c20 localhost:8080' -- npm run prod",
    "start": "node src/server",
    "test": "npm run lint",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robbtraister/react-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robbtraister/react-boilerplate/issues"
  },
  "homepage": "https://github.com/robbtraister/react-boilerplate#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier-standard": "^9.1.1",
    "sass": "^1.22.3",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "terser-webpack-plugin": "^1.3.0",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "yaml-loader": "^0.5.0"
  }
}
